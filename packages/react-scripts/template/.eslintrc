{
    "extends": ["react-app", "@ackee", "prettier"],
    "settings": {
        "import/resolver": {
            "node": {
                "paths": ["src"]
            }
        }
    },
    "rules": {
        "no-restricted-globals": "off"
    },
    "env": {
        "browser": true,
        "jest": true,
        "cypress/globals": true
    },
    "plugins": ["cypress"]
}
